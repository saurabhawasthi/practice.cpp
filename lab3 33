# practice.cpp333
 #include <iostream>
     #include <math.h>
     using namespace std;
     int main()
     {
        int  n,rem1,rem2;
        cout<<"enter a number :\n";
        cin>>n;
        rem1=n%10;
        while(n>10)
        {
            n=n/10;
        }
             rem2=n%10;
             cout<<"sum of 1st digit and last digit is = "<<rem1+rem2<<endl;
             return 0;
    }
