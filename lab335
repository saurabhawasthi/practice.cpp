# practice.cpp35

#include <iostream>
 using namespace std;
 int main()
 {
     int val,n,sum=0;
     cout<<"enter any number :\n";
     cin>>val;
     n=val;
     while(n!=0)
     {
     sum=sum+n%10;
     n=n/10;
     }
     cout<<"sum of digits of given number are = "<<sum<<endl;
     return 0;
    }
