# practice.cpp32
#include <iostream>
    #include <math.h>
    using namespace std;
    int main()
    {
        int n,rem2,rem1;
        cout<<"enter any number :\n";
        cin>>n;
        rem2=n%10;
        while(n>10)
        {
             n=n/10;
        }
             rem1=n%10;
             cout<<"1st digit in entered number is = "<<rem1<<endl;
             cout<<"last digit in entered number is = "<<rem2<<endl;
             return 0;
    }
