  #include <iostream>
   using namespace std;
   int main()
   {
       int percantage,grade;
       cout<<"enter percantage :\n";
       cout<<"enter grade :\n";
       cin>>grade;
       cin>>percantage;
       switch(grade)
       {
            grade 'A':
            cout<<"percantage >=90 :\n";
            break;
            grade 'B':
            cout<<"percantage >=80 :\n";
            break;
            grade 'C':
            cout<<"percantage >=70 :\n";
            break;
            grade 'D':
            cout<<"percantage >=60 :\n";
            break;
            grade 'E':
            cout<<"percantage >=40 :\n";
            break;
            grade 'F';
            cout<<"percantage <40  :\n";
            break;
            default :
            cout<<"invalid choice :\n";
            break;
        }
            return 0;
    }

